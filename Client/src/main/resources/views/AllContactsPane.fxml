<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<AnchorPane fx:id="allContactsAnchorPane"
            styleClass="logo-anchor-pane"
            stylesheets="@../styles/SignUp.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.connectify.controller.AllContactsPaneController">
    <children>
        <TextField fx:id="ContactSearchTextField" blendMode="SCREEN" layoutX="19.0" layoutY="14.0" prefHeight="26.0" prefWidth="311.0" promptText="Enter Contact Name" styleClass="text-field">
            <stylesheets>
                <URL value="@../styles/SignUp.css" />
                <URL value="@../styles/AllContactsPaneStyles.css" />
            </stylesheets></TextField>
        <ScrollPane fx:id="allContactsScrollPane" hbarPolicy="NEVER" layoutY="60.0" minWidth="350.0" prefHeight="940.0" prefWidth="350.0" stylesheets="@../styles/AllContactsPaneStyles.css" vbarPolicy="NEVER">
            <content>
                <VBox fx:id="allContactsVBox" alignment="TOP_CENTER" minWidth="350.0" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
