<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.web.*?>

<HBox fx:id="userMessageBox" alignment="TOP_RIGHT" prefHeight="85.0" prefWidth="392.0" styleClass="user-message" stylesheets="@../styles/Chat.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <WebView prefHeight="65.0" prefWidth="282.0" styleClass="contact-message">
         <HBox.margin>
            <Insets />
         </HBox.margin></WebView>
      <Circle fx:id="contactImage" fill="#4c5660" radius="32.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Circle>
   </children>
   <padding>
      <Insets bottom="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
